var models = require("../app/models");

models.sequelize.sync().then(() => {

  models.Service
    .bulkCreate([
      { name: 'Atención Medica', description: 'Atención Medica.', resource: 'local_hospital', appointmentCatalog: 'health'},
      { name: 'Atención Psicosocial', description: 'Atención Psicosocial.', resource: 'account_balance', appointmentCatalog: 'social'},
      { name: 'Atención Legal', description: 'Atención Legal. ', resource: 'face', appointmentCatalog: 'law'},
      { name: 'Trabajo', description: 'trabajo. ', resource: 'work', appointmentCatalog: 'work'}
    ])
    .then((service) => {
      console.log('service catalog: ' + service.length);
    });
});
